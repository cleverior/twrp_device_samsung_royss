# mount point	fstype		device			[device2]

/boot		emmc		/dev/block/mmcblk0p8
/system		ext4		/dev/block/mmcblk0p21
/data		ext4		/dev/block/mmcblk0p24				length=-16384
/cache		ext4		/dev/block/mmcblk0p22				flags=backup=1;display=cache
/recovery	emmc		/dev/block/mmcblk0p13				flags=backup=1
/efs		ext4		/dev/block/mmcblk0p16				flags=backup=1;display=EFS
/external_sd	vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="SD Card";storage;wipeingui;removable
